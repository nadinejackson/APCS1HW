    */

    public boolean add(Comparable c)
    {
	int insertion;
	for(int i = 0; i < this.size() - 1; i++)
	    {
		if (c < this.get(i + 1)
		    && this.get(i) < c)
		    insertion = i + 1;
	    }
    }
    
    public void add( int index, Comparable c)
    {
	if (c.compareTo(this.get(index - 1)) < 0)
	    return false;
	else
	    this();
    }
    
    public static void main(String [] args)
    {
	
    }
    
}
