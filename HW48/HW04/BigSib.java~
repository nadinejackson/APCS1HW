public class BigSib {
    public static void bigsib( String name ){
	System.out.print("Why hello there, ");
	System.out.print(name);
	System.out.println(". How do you do?");
    }
    public static void main( String[] args ){
	bigsib("Marie Joseph Paul Yves Roch Gilbert du Motier, Marquis de Lafayette");
	bigsib("not Nadine the Bold");
	bigsib("three hardcoded persons of interest");
    }
}
